package exceptions;

/**
 *
 * @author Adrian Rocha
 */
public class ExceptionNotPermited extends Exception {
    /**
	 * 
	 */
	private static final long serialVersionUID = -5947526092816256542L;
	private String mensaje;
 
    public ExceptionNotPermited(String msg) {
        mensaje = msg;
    }
        
    @Override
    public String toString() {
        return mensaje;
    }
}
